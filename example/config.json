{
  "defaults": {
    "cors": {
      "allowOrigin": ["http://localhost:8080"],
      "allowCredentials": true,
      "exposeHeaders": ["Link"]
    }
  },
  "resources": [{
    "path": "/unauthorized",
    "status": 401,
    "headers": {
      "content-type": "application/vnd.api+json",
      "www-authenticate": "ClientBasic",
      "link": "</login>; rel=\"authenticate\"; display=\"authenticate\""
    }
  }]
}
